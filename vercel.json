{
    "builds": [{
        "src": "core/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/core/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "core/stockFlow/wsgi.py"
        }
    ]
}