{
    "version": 2,
    "buildCommand": "sh build_files.bash",
    "outputDirectory": "staticfiles",
    "rewrites": [
        {
            "source": "/static/(.*)",
            "destination": "/staticfiles/$1"
        },
        {
            "source": "/(.*)",
            "destination": "core/wsgi.py"
        }
    ]
}