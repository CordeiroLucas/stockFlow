{
    "version": 2,
    "buildCommand": "python3 manage.py collectstatic --noinput --clear",
    "outputDirectory": "staticfiles",
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "core/wsgi.py"
        }
    ]
}